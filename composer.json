{
    "name": "fuelviews/laravel-navigation",
    "description": "This is my package laravel-navigation",
    "keywords": [
        "Fuelviews",
        "laravel",
        "laravel-navigation"
    ],
    "homepage": "https://github.com/fuelviews/laravel-navigation",
    "license": "MIT",
    "authors": [
        {
            "name": "thejmitchener",
            "email": "support@fuelviews.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:fuelviews/laravel-middleware.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:fuelviews/laravel-form.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0 || ^11.0",
        "illuminate/support": "^10.0 || ^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "ralphjsmit/laravel-glide": "^1.2.1",
        "fuelviews/laravel-form": "0.0.3"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Fuelviews\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuelviews\\Navigation\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-navigation --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fuelviews\\Navigation\\NavigationServiceProvider"
            ],
            "aliases": {
                "Navigation": "Fuelviews\\Navigation\\Facades\\Navigation"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
